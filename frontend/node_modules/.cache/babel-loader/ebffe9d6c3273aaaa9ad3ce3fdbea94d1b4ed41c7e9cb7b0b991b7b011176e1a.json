{"ast":null,"code":"export default {\n  props: {\n    placeholder: String,\n    // 搜索框的占位符\n    items: Array,\n    // 下拉列表数据\n    currentPage: Number,\n    // 当前页码\n    totalPages: Number // 总页数\n  },\n  data() {\n    return {\n      searchQuery: '',\n      // 搜索关键字\n      isClicked: false // 是否点击\n    };\n  },\n  methods: {\n    handleCommand(command) {\n      this.$emit('select', command); // 触发选择事件\n    },\n    onSearch() {\n      this.$emit('search', this.searchQuery); // 触发搜索事件\n    },\n    prevPage() {\n      this.$emit('prev-page'); // 触发上一页事件\n    },\n    nextPage() {\n      this.$emit('next-page'); // 触发下一页事件\n    },\n    onDropdownVisibleChange(visible) {\n      this.isClicked = visible;\n      if (!visible) {\n        this.searchQuery = '';\n      }\n    }\n  }\n};","map":{"version":3,"names":["props","placeholder","String","items","Array","currentPage","Number","totalPages","data","searchQuery","isClicked","methods","handleCommand","command","$emit","onSearch","prevPage","nextPage","onDropdownVisibleChange","visible"],"sources":["E:\\Wuhuairline\\frontend\\src\\components\\SelectDropdown.vue"],"sourcesContent":["<template>\n  <el-dropdown\n    trigger=\"click\"\n    @command=\"handleCommand\"\n    style=\"display:flex;margin-right:2%\"\n    :hide-on-click=\"true\"\n    @visible-change=\"onDropdownVisibleChange\"\n  >\n    <el-button class=\"custom-button\" :class=\"{ clicked: isClicked }\">\n      <i class=\"el-icon-arrow-down\"></i>\n    </el-button>\n    <!-- 下拉菜单 -->\n    <template v-slot:dropdown>\n      <el-dropdown-menu class=\"el-dropdown-menu\">\n        <!-- 搜索框 -->\n        <div class=\"search-item\">\n          <el-input\n            v-model=\"searchQuery\"\n            :placeholder=\"placeholder\"\n            style=\"flex:1;\"\n            @keyup.enter=\"onSearch\"\n          />\n          <button @click=\"onSearch\" style=\"height:80%;background-color: transparent;\">🔍</button>\n        </div>\n        <!-- 列表 -->\n        <el-dropdown-item v-for=\"item in items\" :key=\"item.id\" :command=\"item\" class=\"content-item\">\n          {{ item.name }}\n        </el-dropdown-item>\n        <!-- 翻页控件 -->\n        <div class=\"pagination-item\">\n          <div class=\"pagination-controls\" style=\"display:flex;flex-direction: row;justify-content: center\">\n            <button @click=\"prevPage\" v-if=\"currentPage !== 1\" class=\"pagination-button\">◄</button>\n            <span style=\"color:#68F0EB;font-size:80%\">{{ currentPage }} / {{ totalPages }}</span>\n            <button @click=\"nextPage\" v-if=\"currentPage !== totalPages\" class=\"pagination-button\">►</button>\n          </div>\n        </div>\n      </el-dropdown-menu>\n    </template>\n  </el-dropdown>\n</template>\n\n<script>\nexport default {\n  props: {\n    placeholder: String, // 搜索框的占位符\n    items: Array, // 下拉列表数据\n    currentPage: Number, // 当前页码\n    totalPages: Number, // 总页数\n\n  },\n  data() {\n    return {\n      searchQuery: '', // 搜索关键字\n      isClicked: false, // 是否点击\n    };\n  },\n  methods: {\n    handleCommand(command) {\n      this.$emit('select', command); // 触发选择事件\n    },\n    onSearch() {\n      this.$emit('search', this.searchQuery); // 触发搜索事件\n    },\n    prevPage() {\n      this.$emit('prev-page'); // 触发上一页事件\n    },\n    nextPage() {\n      this.$emit('next-page'); // 触发下一页事件\n    },\n    onDropdownVisibleChange(visible) {\n      this.isClicked = visible;\n      if(!visible){\n         this.searchQuery='';\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.custom-button{\n\n    appearance: none;\n    background-color: transparent;\n    width:1vw;\n    height:1vw;\n\n    border:none;\n    align-self: center;\n\n    margin-right:2%;\n\n    /*指针样式*/\n    cursor: pointer;\n      display: flex;\n      justify-content: center;\n      align-items: center;\n}\n\n.custom-button i {\n    display:inline-block;\n    background-image: url(\"@/assets/DataSimulation/arrow-right.svg\");\n    background-repeat: no-repeat;\n    background-position: center center;\n    background-size: contain;\n    width: 1vw;\n    height: 1vw;\n    transition: transform 0.3s ease; /* 添加旋转动画 */\n}\n\n.custom-button:hover i {\n  transform: rotate(90deg);\n  transition: transform 0.3s ease;\n}\n\n/* 定义点击后的样式 */\n.custom-button.clicked i {\n  transform: rotate(90deg);\n}\n\n\n.el-dropdown-menu {\n    min-width:220px;\n    border:1px solid #68F0EB;\n    background-color: #081028;\n    border-radius:3px;\n    list-style: none;\n    padding: 5%; /* 去掉默认的内边距 */\n    margin: 0; /* 去掉默认的外边距 */\n\n}\n:deep(.content-item){\n    padding:0.3vw 0.3vw;\n    color:#68F0EB;\n    cursor:pointer;\n    transition:background-color 0.3s ease;\n    border: none;\n}\n\n:deep(.content-item:hover ) {\n  background-color:  #1a2a4a; /* 悬停时的背景色 */\n}\n\n\n.pagination-button{\n    background-color: #081028;\n    color:#68F0EB;\n\n}\n\n.search-item{\n    display:flex;\n    justify-content: space-between;\n    align-items: center;\n    width:100%;\n\n}\n\n\n</style>"],"mappings":"AA0CA,eAAe;EACbA,KAAK,EAAE;IACLC,WAAW,EAAEC,MAAM;IAAE;IACrBC,KAAK,EAAEC,KAAK;IAAE;IACdC,WAAW,EAAEC,MAAM;IAAE;IACrBC,UAAU,EAAED,MAAM,CAAE;EAEtB,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MAAE;MACjBC,SAAS,EAAE,KAAK,CAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAACC,OAAO,EAAE;MACrB,IAAI,CAACC,KAAK,CAAC,QAAQ,EAAED,OAAO,CAAC,EAAE;IACjC,CAAC;IACDE,QAAQA,CAAA,EAAG;MACT,IAAI,CAACD,KAAK,CAAC,QAAQ,EAAE,IAAI,CAACL,WAAW,CAAC,EAAE;IAC1C,CAAC;IACDO,QAAQA,CAAA,EAAG;MACT,IAAI,CAACF,KAAK,CAAC,WAAW,CAAC,EAAE;IAC3B,CAAC;IACDG,QAAQA,CAAA,EAAG;MACT,IAAI,CAACH,KAAK,CAAC,WAAW,CAAC,EAAE;IAC3B,CAAC;IACDI,uBAAuBA,CAACC,OAAO,EAAE;MAC/B,IAAI,CAACT,SAAQ,GAAIS,OAAO;MACxB,IAAG,CAACA,OAAO,EAAC;QACT,IAAI,CAACV,WAAW,GAAC,EAAE;MACtB;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}